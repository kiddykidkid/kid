@font-face {
    font-family: "Heartfield";
    src: url('../assets/Heartfield.otf') format('truetype');
}

#enter-container {
    top: 7%;
}

.enter-button {
    transition: opacity 1s ease-in-out;
}


.snowflake {
    position: absolute;
    display: block;
    font-size: 16px;
    color: #2A2A2A;
    user-select: none;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
}